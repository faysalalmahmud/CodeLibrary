#include<bits/stdc++.h>
using namespace std;

int main(int argc, char const *argv[])
{
    string S;
    cin>>S;
    int lost_count=0;

    for (int i = 0; i < S.length(); i++)
    {

        if (S.at(i)=='x')
        {
            lost_count++;
        }
        
    }
    if (lost_count<15-lost_count)
    {
        cout<<"YES"<<endl;
    }
    else{
        cout<<"NO"<<endl;
    }
    
    
    return 0;
}


